{
  "name": "js-portfolio",
  "version": "1.0.0",
  "description": "A minimal portfolio with Vanilla JavaScript",
  "main": "index.js",
  "scripts": {
    "build:watch": "webpack --config webpack.config.dev.js --watch",
    "serve:dev": "webpack --config webpack.config.dev.js && npx serve dist -s",
    "serve:prod": "webpack --config webpack.config.js && npx serve dist -s",
    "prebuild": "node ./scripts/create-env.js",
    "build:analyze": "webpack --analyze",
    "build": "webpack --config webpack.config.js",
    "dev": "webpack serve --config webpack.config.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gndx/js-portfolio.git"
  },
